    "pear-pear.php.net/Console_Color": "@stable",
    "pear-pear.php.net/Console_Table": "@stable",
    "pear-pear.php.net/File_SearchReplace": "@stable",
    "pear-pear.php.net/HTML_CSS": "@stable",
    "pear-pear.php.net/Net_Server": "@stable",
    "pear-pear.php.net/Services_JSON": "@stable"
  },
  "extra": {
    "branch-alias": {
      "dev-develop-2": "0.9.x-dev"
    }
