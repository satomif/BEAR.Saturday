{
  "name": "bearsaturday/bearsaturday",
  "description": "BEAR.Saturday framework for PHP 5.2+",
  "keywords": [
    "Framework",
    "BEAR.Saturday"
  ],
  "license": "BSD-3-Closure",
  "authors": [
    {
      "name": "Akihito Koriyama",
      "email": "akihito.koriyama@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    },
    {
      "type": "pear",
      "url": "http://bearsaturday.github.io/pear"
